  <body>
      <% include ./partials/nav %>
     
              
              <%- body %>
		
    <% include ./partials/footer %>
    <script>
        $(document).ready(function() {
        
        $(".owl-carousel").owlCarousel({
        
        autoPlay: 3000,
        items : 4,
        itemsDesktop : [1199,3],
        itemsDesktopSmall : [979,3],
        center: true,
        nav:true,
        loop:true,
        responsive: {
        600: {
        items: 4
        }
        }
        });        
        });
     </script>
     <script>
        $(document).on("scroll", function(){
          if
             ($(document).scrollTop() > 86){
            $("#tks").addClass("fixed");
          }
          else
          {
            $("#tks").removeClass("fixed");
          }
        });
        
     </script>
  </body>
</html>
