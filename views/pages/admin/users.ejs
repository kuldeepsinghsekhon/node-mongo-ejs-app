<div class="row">
        <div class="col-md-12 grid-margin">
                <div class="d-flex justify-content-between align-items-center">
                  <div>
                    <h4 class="font-weight-bold mb-0">Users Mangement</h4>
                  </div>
                  <!-- <div>
                      <button type="button" class="btn btn-primary btn-icon-text btn-rounded">
                        <i class="ti-clipboard btn-icon-prepend"></i>AddUsers
                      </button>
                  </div> -->
                </div>
              </div>
        <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">All Users</h4>
                    <p class="card-description">
                      Manage <code>users</code>
                    </p>
                    <div class="table-responsive">
                      <div id="userAction"></div>
                      <table class="table table-striped" id="user-data">
                        <thead>
                          <tr>
                           
                            <th>User Image </th>
                            <th> Full name </th>
                            <th>Email</th>
                            <th>Status</th>
                           
                          </tr>
                        </thead>
                        <tbody>
                            <% for (var i = 0; i < users.length; i++) { %>
                          <tr>
                          
                            <td class="py-1"><img src="/assets/images/defaultuser.png " alt="image"/></td>
                            <td><%= users[i].name %></td>
                            <td> <%= users[i].email %> </td>
                            <td>
                                  <label class="switch">
                                     <input id="user_status" data-uid="<%= users[i].id%>" type="checkbox" onclick="return confirm('Aru You Sure to Want Change Status ?')" data-id="2" name="status" class="usersatusbtn" value="true" <% if(users[i].status){if(users[i].status){ %> checked <% }} %>>
                                     <span class="slider round"></span>
                                  </label>
                            </td>
                          </tr>
                         <% } %>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
